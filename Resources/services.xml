<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="shopgate_cloudapi.client_credentials" class="ShopgateCloudApi\Components\ClientCredentials">
            <argument type="service" id="config"/>
        </service>
        <service id="shopgate_cloudapi.repo_sdk_token" class="ShopgateCloudApi\Repositories\Sdk\Token">
            <argument type="service" id="dbal_connection"/>
        </service>
        <service id="shopgate_cloudapi.endpoint_subscriber" class="ShopgateCloudApi\Subscriber\EndpointSubscriber">
            <tag name="shopware.event_subscriber"/>
        </service>
    </services>
</container>
